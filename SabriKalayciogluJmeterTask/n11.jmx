          <stringProp name="WebDriverSampler.script">import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.ui.WebDriverWait;
import org.openqa.selenium.support.ui.ExpectedConditions;
import java.time.Duration;
import java.util.concurrent.TimeUnit;
//
// Click seearchbox
// Wait for search results to appear
// Click first link in &quot;popular searches&quot; listed
// Check that products are listed
// Time the search
//
WDS.browser.get(&apos;http://n11.com&apos;);
WebDriverWait wait = new WebDriverWait(WDS.browser, Duration.ofSeconds(20));
wait.until(ExpectedConditions.elementToBeClickable(By.xpath(&quot;//input[@id=&apos;searchData&apos;]&quot;))).click();
wait.until(ExpectedConditions.elementToBeClickable(By.xpath(&quot;//div[@class=&apos;srch-private-most srch-suggestion&apos;]&quot;)));
Thread.sleep(1000); // just to make the run easier to follow, otherwise not needed
WebElement firstSuggestion = WDS.browser.findElement(By.xpath(&quot;//div[@class=&apos;srch-private-most srch-suggestion&apos;]/ul/li[1]&quot;));
WDS.sampleResult.sampleStart();
firstSuggestion.click();
wait.until(ExpectedConditions.presenceOfElementLocated(By.className(&quot;productArea&quot;)));
WDS.sampleResult.sampleEnd();</stringProp>
